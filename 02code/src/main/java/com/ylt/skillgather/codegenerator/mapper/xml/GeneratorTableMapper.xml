<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ylt.skillgather.codegenerator.mapper.GeneratorTableMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ylt.skillgather.codegenerator.entity.GeneratorTable">
        <id column="ID" property="id" />
        <result column="TableName" property="TableName" />
        <result column="MenuName" property="MenuName" />
        <result column="IsCreat" property="IsCreat" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, TableName, MenuName, IsCreat
    </sql>

    <select id="excuteSql" parameterType="java.lang.String"   resultType="java.lang.Integer">
      ${sql}
    </select>


</mapper>
